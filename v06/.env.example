# Example environment variables for Firebase (DO NOT COMMIT real keys here)
# Copy this file to `.env` and fill in your project's values.

# Web
FIREBASE_API_KEY_WEB=your-web-api-key
FIREBASE_APP_ID_WEB=your-web-app-id
FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_AUTH_DOMAIN=your-auth-domain
FIREBASE_STORAGE_BUCKET=your-storage-bucket

# Android
FIREBASE_API_KEY_ANDROID=your-android-api-key
FIREBASE_APP_ID_ANDROID=your-android-app-id

# iOS
FIREBASE_API_KEY_IOS=your-ios-api-key
FIREBASE_APP_ID_IOS=your-ios-app-id
FIREBASE_IOS_BUNDLE_ID=your.ios.bundle.id

# Windows
FIREBASE_API_KEY_WINDOWS=your-windows-api-key
FIREBASE_APP_ID_WINDOWS=your-windows-app-id
FIREBASE_AUTH_DOMAIN_WINDOWS=your-auth-domain
FIREBASE_STORAGE_BUCKET_WINDOWS=your-storage-bucket